<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.edu.hit.violetsns.Mapper.PostThumbMapper">
    <select id="findByPostId" resultType="cn.edu.hit.violetsns.Entity.pojo.PostThumbs">
        select * from post_thumbs where post_id = #{postId}
    </select>
</mapper>